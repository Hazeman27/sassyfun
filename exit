[32m+[m[32m            unraveledItemChildrenSufixes = new Array;[m
[32m+[m
[32m+[m[32m            unraveledItemChildren.forEach(child => {[m
[32m+[m[32m                unraveledItemChildrenSufixes.push(child.className.replace(containerClassName, ''));[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            unraveledItemChildren.forEach((child, index) => {[m
