[32m+[m[32mclass Carousel {[m
[32m+[m
[32m+[m[32m    constructor(container, nextSlideButton, prevSlideButton, slideAnimationDuration) {[m
[32m+[m
[32m+[m[32m        this.slideAnimationDuration = slideAnimationDuration;[m
[32m+[m[32m        this.slidesTrack = container;[m
[32m+[m[32m        this.nextSlideButton = nextSlideButton;[m
[32m+[m[32m        this.prevSlideButton = prevSlideButton;[m
[32m+[m
[32m+[m[32m        this.slides = Array.from(this.slidesTrack.children);[m
[32m+[m[41m        [m
[32m+[m[32m        this.slidesAmount = this.slides.length;[m
[32m+[m[32m        this.slideWidth = this.slides[0].getBoundingClientRect().width;[m
[32m+[m[41m        [m
[32m+[m		[32mthis.indicatorsList = this.slidesTrack.parentElement.appendChild(document.createElement('div'));[m
[32m+[m[32m        this.indicators = new Array;[m
[32m+[m
[32m+[m[32m        this.currentSlide = null;[m
[32m+[m[32m        this.currentSlideIndex = null;[m
[32m+[m[32m        this.currentIndicator = null;[m
[32m+[m[32m        this.targetSlide = null;[m
[32m+[m[32m        this.targetSlideIndex = null;[m
[32m+[m		[32mthis.targetIndicator = null;[m
[32m+[m[41m		[m
[32m+[m		[32mthis.targetIsOutOfBounds = false;[m
[32m+[m		[32mthis.onGoingAnimation = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    createCarouselSlidesClasses() {[m
[32m+[m
[32m+[m[32m        this.slides.forEach(slide => {[m
[32m+[m[32m            slide.classList.add('carousel__slide');[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        this.slides[0].classList.add('carousel__slide_current');[m
[32m+[m[32m        this.targetSlideIndex = 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    createIndicators() {[m
[32m+[m
[32m+[m[32m        this.indicatorsList.classList.add('carousel__indicators');[m
[32m+[m
[32m+[m[32m        for (let i = 0; i < this.slidesAmount; i++)[m
[32m+[m[32m            this.indicators.push(document.createElement('div'));[m
[32m+[m[41m    [m
[32m+[m[32m        this.indicators.forEach((indicator) => {[m
[32m+[m
[32m+[m[32m            indicator.classList.add('carousel__indicator');[m
[32m+[m[32m            this.indicatorsList.appendChild(indicator);[m
[32m+[m[32m        });[m
[32m+[m[41m    [m
[32m+[m[32m        this.indicators[0].classList.add('carousel__indicator_current');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    positionSlides(currentSlideIndex, slideWidth) {[m
[32m+[m
[32m+[m[32m        for (let i = 0; i < this.slidesAmount; i++) {[m
[32m+[m
[32m+[m[32m            if (i == currentSlideIndex) {[m
[32m+[m[41m                [m
[32m+[m[32m                this.slides[currentSlideIndex].style.left = 0 + 'px';[m
[32m+[m[32m                continue;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            this.slides[i].style.left = slideWidth + 'px';[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    reassignCurrentSlide() {[m
[32m+[m[41m    [m
[32m+[m[32m        this.currentSlide.classList.remove('carousel__slide_current');[m
[32m+[m[32m        this.targetSlide.classList.add('carousel__slide_current');[m
[32m+[m[41m        [m
[32m+[m[32m        this.currentIndicator.classList.remove('carousel__indicator_current');[m
[32m+[m[32m        this.targetIndicator.classList.add('carousel__indicator_current');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    moveToSlide() {[m
[32m+[m
[32m+[m[32m        this.targetSlideIndex = this.slides.findIndex(slide => slide === this.targetSlide);[m
[32m+[m[32m        this.currentSlideIndex = this.slides.findIndex(slide => slide === this.currentSlide);[m
[32m+[m[41m    [m
[32m+[m[32m        if ((this.targetSlideIndex > this.currentSlideIndex)[m
[32m+[m[32m                && !this.targetIsOutOfBounds || this.targetIsOutOfBounds && this.targetSlideIndex == 0) {[m
[32m+[m[41m        [m
[32m+[m[32m            this.targetSlide.style.left = this.slideWidth + 'px';[m
[32m+[m[32m            imagesLoaded(this.slides, this.animate('-='));[m
[32m+[m[32m        }[m
[32m+[m[32m        else if ((this.targetSlideIndex < this.currentSlideIndex)[m
[32m+[m[32m                && !this.targetIsOutOfBounds || this.targetIsOutOfBounds && this.targetSlideIndex == this.slidesAmount - 1) {[m
[32m+[m
[32m+[m[32m            this.targetSlide.style.left = -this.slideWidth + 'px';[m
[32m+[m[32m            imagesLoaded(this.slides, this.animate('+='));[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.reassignCurrentSlide();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    animate(direction) {[m
[32m+[m
[32m+[m[32m        this.onGoingAnimation = true;[m
[32m+[m
[32m+[m[32m        $(this.currentSlide).animate({[m
[32m+[m[32m            left: direction + this.slideWidth[m
[32m+[m[32m        },[m
[32m+[m[32m        {[m
[32m+[m[32m            duration: this.slideAnimationDuration,[m
[32m+[m[32m            easing: 'swing',[m
[32m+[m[32m            start: () => {[m
[32m+[m[32m                $(this.targetSlide).animate({[m
[32m+[m[32m                    left: direction + this.slideWidth[m
[32m+[m[32m                },[m
[32m+[m[32m                {[m
[32m+[m[32m                    duration: this.slideAnimationDuration,[m
[32m+[m[32m                    easing: 'swing',[m
[32m+[m[32m                    complete: () => {[m
[32m+[m[32m                        this.currentSlide.style.left = this.slideWidth + 'px';[m
[32m+[m[32m                        this.onGoingAnimation = false;[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    getCurrentAndTargetSlide(direction) {[m
[32m+[m
[32m+[m[32m        this.currentSlide = this.slidesTrack.querySelector('.carousel__slide_current');[m
[32m+[m[32m        this.currentIndicator = this.indicatorsList.querySelector('.carousel__indicator_current');[m
[32m+[m[41m    [m
[32m+[m[32m        if (direction == 'forward') {[m
[32m+[m
[32m+[m[32m            this.targetSlide = this.currentSlide.nextElementSibling;[m
[32m+[m[32m            this.targetIndicator = this.currentIndicator.nextElementSibling;[m
[32m+[m
[32m+[m[32m            if (!this.targetSlide) {[m
[32m+[m[41m                [m
[32m+[m[32m                this.targetSlide = this.slides[0];[m
[32m+[m[32m                this.targetIndicator = this.indicators[0];[m
[32m+[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        else if (direction == 'backward') {[m
[32m+[m
[32m+[m[32m            this.targetSlide = this.currentSlide.previousElementSibling;[m
[32m+[m[32m            this.targetIndicator = this.currentIndicator.previousElementSibling;[m
[32m+[m
[32m+[m[32m            if (!this.targetSlide) {[m
[32m+[m[41m            [m
[32m+[m[32m                this.targetSlide = this.slides[this.slidesAmount - 1];[m
[32m+[m[32m                this.targetIndicator = this.indicators[this.slidesAmount - 1];[m
[32m+[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        else {[m
[32m+[m
[32m+[m[32m            this.targetSlide = this.slides[direction];[m
[32m+[m[32m            this.targetIndicator = this.indicators[direction];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    slide(direction) {[m
[32m+[m
[32m+[m[32m        this.targetIsOutOfBounds = this.getCurrentAndTargetSlide(direction);[m
[32m+[m[32m        this.moveToSlide(this.targetIsOutOfBounds);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// ===========================[m
[32m+[m
[32m+[m[32m$(document).ready(() => {[m
[32m+[m
[32m+[m	[32mconst slidesTrack = document.querySelector('.carousel__slides-container');[m
[32m+[m	[32mconst nextSlideButton = document.querySelector('.carousel__button_right');[m
[32m+[m	[32mconst prevSlideButton = document.querySelector('.carousel__button_left');[m
[32m+[m
[32m+[m	[32mconst carousel = new Carousel(slidesTrack, nextSlideButton, prevSlideButton, 450);[m
[32m+[m
[32m+[m	[32mcarousel.createCarouselSlidesClasses();[m
[32m+[m	[32mcarousel.createIndicators();[m
[32m+[m	[32mcarousel.positionSlides(0, carousel.slideWidth);[m
[32m+[m
[32m+[m	[32m$(window).resize(() => {[m
