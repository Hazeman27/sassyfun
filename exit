[32m+[m[32m$(document).ready(() => {[m
[32m+[m
[32m+[m[32m    const containerClassName = 'masonry-grid__item';[m
[32m+[m[32m    const items = document.getElementsByClassName(containerClassName);[m
[32m+[m
[32m+[m[32m    var unraveled = false;[m
[32m+[m[32m    var unraveledItem = null;[m
[32m+[m[32m    var unraveledItemChildren, unraveledItemChildrenSufixes;[m
[32m+[m
[32m+[m[32m    $(items).hover(function() {[m
[32m+[m
[32m+[m[32m        this.addEventListener('click', e => {[m
[32m+[m[32m            $(this.children[4]).css('filter', 'brightness(100%)');[m
[32m+[m[32m        });[m
[32m+[m[32m        if (unraveled) return;[m
[32m+[m[32m        else $(this.children[4]).css('filter', 'brightness(65%)');[m
[32m+[m[32m    }, function() {[m
[32m+[m[32m        $(this.children[4]).css('filter', 'brightness(100%)');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    $(items).click(function() {[m
[32m+[m
[32m+[m[32m        if (unraveled) {[m
[32m+[m
[32m+[m[32m            unraveledItem.classList.remove(unraveledItem.classList[1]);[m
[32m+[m[32m            unraveledItemChildren.forEach(child => { child.classList.remove(child.classList[1]); });[m
[32m+[m[32m            unraveled = false;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        else {[m
[32m+[m
[32m+[m[32m            this.classList.add(this.className + '_unraveled');[m
[32m+[m[41m            [m
[32m+[m[32m            unraveledItem = this;[m
[32m+[m[32m            unraveled = true;[m
[32m+[m
